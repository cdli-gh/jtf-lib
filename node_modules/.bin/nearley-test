#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../uqnu/uqnu/node_modules/nearley/bin/nearley-test.js" "$@"
  ret=$?
else 
  node  "$basedir/../../../uqnu/uqnu/node_modules/nearley/bin/nearley-test.js" "$@"
  ret=$?
fi
exit $ret
